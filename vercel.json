{
    "builds": [
        {
            "src": "netflix_site/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"  // Specify a supported Python version
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "netflix_site/wsgi.py"
        }
    ]
}
